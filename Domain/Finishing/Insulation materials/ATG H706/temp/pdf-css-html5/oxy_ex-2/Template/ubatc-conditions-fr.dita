                <colspec colname="c1" colnum="1" colwidth="1cm" class="- topic/colspec " xtrf="file:/C:/Users/cch/AppData/Local/Temp/OxygenXMLTemp/ditaTemporaryOutputDir-u1l_wf2_gzb/t1/t2/t3/t4/t5/oxy_ex-2/Template/ubatc-conditions-fr.dita" xtrc="colspec:1;9:66"/>
